CREATE TABLE Tutor ( 
	id_tutor INTEGER GENERATED BY DEFAULT AS IDENTITY PRIMARY KEY,
	nome VARCHAR (100),
	endereco VARCHAR(150),
	telefone NUMERIC (15),
	email VARCHAR (50)
	)
	
