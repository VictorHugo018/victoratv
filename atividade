CREATE TABLE Exames (
	id_Exame INTEGER GENERATED BY DEFAULT AS IDENTITY PRIMARY KEY,
	data_exame timestamp,
	tipo_exame VARCHAR(100),
	resultado varchar (50),
	id_paciente INTEGER,
	FOREIGN KEY (id_paciente) REFERENCES Paciente (id_paciente)
)


