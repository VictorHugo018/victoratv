CREATE TABLE Paciente ( 
	id_paciente INTEGER GENERATED BY DEFAULT AS IDENTITY PRIMARY KEY,
	nome VARCHAR (100),
	especie  VARCHAR(80),
	raca  NUMERIC (15),
	idade INT,
	peso numeric(3,2),
	id_tutor integer, 
 FOREIGN KEY (id_Tutor) REFERENCES Tutor (id_Tutor)

)

