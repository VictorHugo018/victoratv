CREATE TABLE Itens_de_Receita ( 
	id_Itens_de_Receita INTEGER GENERATED BY DEFAULT AS IDENTITY PRIMARY KEY,
	id_receita integer, 
FOREIGN KEY (id_receita) REFERENCES receita (id_receita),
	id_medicamentos integer, 
FOREIGN KEY (id_medicamentos) REFERENCES medicamentos (id_medicamentos),
	quantidade NUMERIC (3,2),
	dosagem  NUMERIC (3,2)
	
)

